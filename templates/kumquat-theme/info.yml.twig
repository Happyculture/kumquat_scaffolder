name: {{ theme }}
type: theme
description: '{{ theme }} theme'
package: {{ profile }}
core_version_requirement: ^10
base theme: stable9

regions:
  branding: Branding
  navigation: Navigation
  highlighted: Highlighted
  help: Help
  content: Content
  footer: Footer
  # Hidden regions.
  page_bottom: 'Page bottom'
  page_top: 'Page top'

libraries:
  - {{ machine_name }}_theme/global
  - {{ machine_name }}_theme/off_canvas_menu
  - core/normalize

ckeditor_stylesheets:
  - dist/css/ckeditor.css
