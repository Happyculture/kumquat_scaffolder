name: {{ name }} Admin Theme
type: theme
description: '{{ name }} administration theme'
package: {{ name }}
core_version_requirement: ^8 || ^9 || ^10
base theme: {{ base_admin_theme }}

libraries:
  - {{ machine_name }}_admin_theme/global

# Regions.
regions:
  header: 'Header'
  pre_content: 'Pre-content'
  breadcrumb: Breadcrumb
  highlighted: Highlighted
  help: Help
  content: Content
  page_top: 'Page top'
  page_bottom: 'Page bottom'
  sidebar_first: 'First sidebar'
regions_hidden:
  - sidebar_first
